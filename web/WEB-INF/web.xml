    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GoodGradeServlet</servlet-name>
        <url-pattern>/GoodGradeServlet</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>login.jsp</welcome-file>
    </welcome-file-list>
